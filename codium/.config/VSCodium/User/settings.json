{
  "workbench.colorTheme": "Black Waves",
  "workbench.iconTheme": "keen-neutral-icon-theme",
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": true,
  "workbench.editor.untitled.hint": "hidden",
  "editor.fontFamily": "'Borg Sans Mono', Consolas, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.fastScrollSensitivity": 4,
  "editor.mouseWheelScrollSensitivity": 1,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.cursorBlinking": "solid",
  "editor.hover.enabled": true,
  "editor.hover.delay": 200,
  "editor.bracketPairColorization.enabled": false,
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "node_modules": true,
    "**/target": true
  },
  "files.saveConflictResolution": "overwriteFileOnDisk",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/bin/bash"
    }
  },
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.easymotionMarkerForegroundColorOneChar": "#569cd6",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#ffffff",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#cccccc",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 200,
  "vim.highlightedyank.color": "#ccc",
  "vim.highlightedyank.textColor": "#000",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-j>": false,
    "<C-w>": false,
    "<C-s>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-n>": false,
    "<C-g>": false,
    "<C-b>": false,
    "<C-[>": false,
    "<C-]>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [" "],
      "after": ["leader", "leader", "leader", "b", "d", "w"]
    }
  ],
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "git.autofetch": true,
  "rust.mode": "legacy",
  "rust.rustup": {
    "toolchain": "stable-x86_64-unknown-linux-gnu"
  },
  "rust-analyzer.checkOnSave.enable": false,
  "rust-analyzer.inlayHints.maxLength": 25,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.closureReturnTypeHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "go.formatTool": "goimports",
  "html.format.wrapLineLength": 85,
  "markdown.extension.toc.updateOnSave": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
  "omnisharp.useGlobalMono": "always",
  "svelte.enable-ts-plugin": true
}
